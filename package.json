{
  "name": "number-guessing-game",
  "version": "1.0.0",
  "description": "A CLI-based number guessing game with TypeScript",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
